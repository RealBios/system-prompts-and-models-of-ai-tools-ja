{
  "tools": [
    {
      "type": "function",
      "function": {
        "name": "view",
        "description": "View a ファイル or ディレクトリ. For ファイル, optionally 検索します within the ファイル using a regex pattern or limit to a line range. Exclude the 'electron' フォルダ by 既定値 unless explicitly requested.",
        "parameters": {
          "type": "object",
          "properties": {
            "type": {
              "type": "string",
              "enum": [
                "file",
                "directory"
              ],
              "description": "Whether to view a single ファイル or a ディレクトリ listing (up to 2 levels)."
            },
            "path": {
              "type": "string",
              "description": "パス relative to the リポジトリ root."
            },
            "view_range": {
              "type": "array",
              "items": {
                "type": "integer"
              },
              "minItems": 2,
              "maxItems": 2,
              "description": "Optional [start_line, end_line] 1-based inclusive range for ファイル."
            },
            "search_query_regex": {
              "type": "string",
              "description": "Optional regex to 検索します within ファイル content (single-line regex)."
            },
            "case_sensitive": {
              "type": "boolean",
              "default": false,
              "description": "Whether the regex 検索します is case-sensitive."
            },
            "context_lines_before": {
              "type": "integer",
              "default": 5,
              "description": "Lines of context to include before each regex match."
            },
            "context_lines_after": {
              "type": "integer",
              "default": 5,
              "description": "Lines of context to include after each regex match."
            }
          },
          "required": [
            "type",
            "path"
          ],
          "additionalProperties": false
        }
      }
    },
    {
      "type": "function",
      "function": {
        "name": "grep-search",
        "description": "検索します across multiple ファイル/directories or the whole codebase. Use for finding text/symbols across many ファイル. Excludes 'electron/**' by 既定値 unless explicitly overridden.",
        "parameters": {
          "type": "object",
          "properties": {
            "query": {
              "type": "string",
              "description": "Text or regex to 検索します for."
            },
            "paths": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "description": "Optional 一覧を取得します of directories or ファイル to limit the 検索します scope."
            },
            "include_globs": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "description": "Optional glob patterns to include (e.g., 'src/**/*.ts')."
            },
            "exclude_globs": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "default": [
                "electron/**"
              ],
              "description": "Optional glob patterns to exclude. Defaults to excluding the 'electron' フォルダ."
            },
            "case_sensitive": {
              "type": "boolean",
              "default": false,
              "description": "Case sensitivity for the 検索します."
            },
            "context_lines_before": {
              "type": "integer",
              "default": 5,
              "description": "Lines of context before each match."
            },
            "context_lines_after": {
              "type": "integer",
              "default": 5,
              "description": "Lines of context after each match."
            },
            "max_results": {
              "type": "integer",
              "default": 5000,
              "description": "Limit the number of matches returned."
            }
          },
          "required": [
            "query"
          ],
          "additionalProperties": false
        }
      }
    },
    {
      "type": "function",
      "function": {
        "name": "codebase-retrieval",
        "description": "High-level retrieval across the current codebase to locate relevant ファイル, classes, functions, or patterns when you don't know where to look.",
        "parameters": {
          "type": "object",
          "properties": {
            "information_request": {
              "type": "string",
              "description": "Natural-language description of what you need to find."
            }
          },
          "required": [
            "information_request"
          ],
          "additionalProperties": false
        }
      }
    },
    {
      "type": "function",
      "function": {
        "name": "git-commit-retrieval",
        "description": "Use the リポジトリ’s コミット history to find how similar changes were made in the past or why changes happened.",
        "parameters": {
          "type": "object",
          "properties": {
            "information_request": {
              "type": "string",
              "description": "Question about past changes (e.g., how/why a feature was implemented)."
            }
          },
          "required": [
            "information_request"
          ],
          "additionalProperties": false
        }
      }
    },
    {
      "type": "function",
      "function": {
        "name": "str-replace-editor",
        "description": "Edit existing ファイル safely. Use 'str_replace' for in-place replacements with explicit line ranges, or 'insert' to insert new content at a specific line.",
        "parameters": {
          "type": "object",
          "properties": {
            "command": {
              "type": "string",
              "enum": [
                "str_replace",
                "insert"
              ],
              "description": "Edit mode: 'str_replace' or 'insert'."
            },
            "path": {
              "type": "string",
              "description": "パス of the ファイル to edit, relative to repo root."
            },
            "instruction_reminder": {
              "type": "string",
              "description": "Must be exactly: 'ALWAYS BREAK DOWN EDITS INTO SMALLER CHUNKS OF AT MOST 150 LINES EACH.'"
            },
            "insert_line_1": {
              "type": "integer",
              "description": "For 'insert': 1-based line number after which to insert. Use 0 to insert at the very beginning."
            },
            "new_str_1": {
              "type": "string",
              "description": "For 'str_replace' and 'insert': the new content."
            },
            "old_str_1": {
              "type": "string",
              "description": "For 'str_replace': the exact original text to replace (must match exactly, including whitespace)."
            },
            "old_str_start_line_number_1": {
              "type": "integer",
              "description": "For 'str_replace': 1-based start line of old_str_1."
            },
            "old_str_end_line_number_1": {
              "type": "integer",
              "description": "For 'str_replace': 1-based end line of old_str_1 (inclusive)."
            }
          },
          "required": [
            "command",
            "path",
            "instruction_reminder"
          ],
          "additionalProperties": false
        }
      }
    },
    {
      "type": "function",
      "function": {
        "name": "save-file",
        "description": "作成します a new ファイル. Does not modify existing ファイル.",
        "parameters": {
          "type": "object",
          "properties": {
            "instructions_reminder": {
              "type": "string",
              "description": "Must be exactly: 'LIMIT THE ファイル CONTENT TO AT MOST 300 LINES. IF MORE CONTENT NEEDS TO BE ADDED USE THE str-replace-editor ツール TO EDIT THE ファイル AFTER IT HAS BEEN CREATED.'"
            },
            "path": {
              "type": "string",
              "description": "パス for the new ファイル, relative to repo root."
            },
            "file_content": {
              "type": "string",
              "description": "Content to 書き込みます into the new ファイル."
            },
            "add_last_line_newline": {
              "type": "boolean",
              "default": true,
              "description": "Whether to ensure a trailing newline."
            }
          },
          "required": [
            "instructions_reminder",
            "path",
            "file_content"
          ],
          "additionalProperties": false
        }
      }
    },
    {
      "type": "function",
      "function": {
        "name": "remove-files",
        "description": "削除します ファイル from the workspace in a reversible way.",
        "parameters": {
          "type": "object",
          "properties": {
            "file_paths": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "description": "一覧を取得します of ファイル パス to remove, relative to repo root."
            }
          },
          "required": [
            "file_paths"
          ],
          "additionalProperties": false
        }
      }
    },
    {
      "type": "function",
      "function": {
        "name": "launch-process",
        "description": "Run a shell コマンド. Use wait=true for short コマンド. OS is win32; shell is 'bash'.",
        "parameters": {
          "type": "object",
          "properties": {
            "command": {
              "type": "string",
              "description": "The shell コマンド to 実行します."
            },
            "wait": {
              "type": "boolean",
              "description": "Whether to wait for the process to complete."
            },
            "max_wait_seconds": {
              "type": "integer",
              "description": "Timeout in seconds when wait=true."
            },
            "cwd": {
              "type": "string",
              "description": "Absolute working ディレクトリ for the コマンド."
            }
          },
          "required": [
            "command",
            "wait",
            "max_wait_seconds",
            "cwd"
          ],
          "additionalProperties": false
        }
      }
    },
    {
